import java.util.*;

class Grade{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[] a=new int[n];

		for(int i=0;i<n;i++)
		{
			a[i]=sc.nextInt();
		}

		for(int i=0;i<n;i++)
		{
			if(a[i]>=38)
			{
				if((((Math.ceil((double)a[i]/5))*5)-a[i])<3)
				{
					System.out.println((int)(Math.ceil((double)a[i]/5)*5));
				}

				else
					System.out.println(a[i]);
			}

			else{
				System.out.println(a[i]);
			}
		}
	}
}
